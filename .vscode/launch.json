            "console": "integratedTerminal"
        },
        {
            "name": "Train GMM model",
            "type": "python",
            "request": "launch",
            "program": "gmm/train_gmm_model.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Evaluate GMM model",
            "type": "python",
            "request": "launch",
            "program": "peg/peg_v2_gmm.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Evaluate SAC-GMM model",
            "type": "python",
            "request": "launch",
            "program": "SAC_GMM/sac_gmm_test.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "SAC GMM",
            "type": "python",
            "request": "launch",
            "program": "SAC_GMM/sac_gmm_test.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: File directory",
            "type": "python",
            "request": "launch",
